    <declare-styleable name="CustomTintEditText">
        <attr name="android:padding"/>
        <attr name="android:paddingLeft"/>
        <attr name="android:paddingRight"/>
        <attr name="android:paddingTop"/>
        <attr name="android:paddingBottom"/>
        <!-- 是否带有边框 -->
        <attr name="tet_isBorderView" format="boolean"/>
        <!-- 普通状态时的边框颜色 -->
        <attr name="tet_borderColor" format="color|reference"/>
        <!-- 含有焦点时的边框颜色 -->
        <attr name="tet_borderFocusColor" format="color|reference"/>
        <!-- 边框圆角大小 -->
        <attr name="tet_borderCornerRadius" format="dimension"/>
        <!-- 边框宽度大小 -->
        <attr name="tet_borderStrokeWidth" format="dimension"/>
        <!-- EditText背景颜色 -->
        <attr name="tet_backgroundColor" format="color|reference"/>

        <!-- 功能的类型 -->
        <attr name="tet_funcType" format="enum">
            <enum name="normal" value="0"/>
            <enum name="canClear" value="1"/>
            <enum name="canWatchPwd" value="2"/>
        </attr>
        <attr name="tet_clearIcon" format="reference"/>
        <!-- 查看密码时的图标 -->
        <attr name="tet_pwdShowIcon" format="reference"/>
        <!-- 隐藏密码时的图标 -->
        <attr name="tet_pwdHideIcon" format="reference"/>
        <!-- 左边图标颜色 -->
        <attr name="tet_leftIconTint" format="color|reference"/>
        <!-- 右边图标颜色 -->
        <attr name="tet_rightIconTint" format="color|reference"/>
        <!-- 图标宽度 -->
        <attr name="tet_iconWidth" format="dimension"/>
        <!-- 图标高度 -->
        <attr name="tet_iconHeight" format="dimension"/>
    </declare-styleable>
